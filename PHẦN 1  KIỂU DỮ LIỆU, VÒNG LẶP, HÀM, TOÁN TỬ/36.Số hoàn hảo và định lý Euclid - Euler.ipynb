    "import math\n",
    "# số hoàn hảo\n",
    "def perfect(n):\n",
    "    tong = 1\n",
    "    for i in range(2,math.isqrt(n)+1):\n",
    "        if n % i == 0:\n",
    "            tong +=i\n",
    "            if n != n//i:\n",
    "                tong+=n // i\n",
    "    return tong == n\n",
